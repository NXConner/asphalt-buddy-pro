# Frontend API base (optional; dev proxy in vite.config.ts points to this)
VITE_API_URL=http://localhost:3001

# Mapbox token for AI Detection map (required for Mapbox map)
# Get one at https://account.mapbox.com/
VITE_MAPBOX_TOKEN=pk.your_public_token_here

# Optional: Contact email for Nominatim (improves geocoding reliability)
VITE_NOMINATIM_EMAIL=you@example.com

# County WMS defaults (optional). If URL and LAYERS are set, layer auto-enables.
# Patrick County, VA
VITE_WMS_PATRICK_URL=
VITE_WMS_PATRICK_LAYERS=
VITE_WMS_PATRICK_VERSION=1.3.0
VITE_WMS_PATRICK_STYLES=

# Henry County, VA
VITE_WMS_HENRY_URL=
VITE_WMS_HENRY_LAYERS=
VITE_WMS_HENRY_VERSION=1.3.0
VITE_WMS_HENRY_STYLES=

# Stokes County, NC
VITE_WMS_STOKES_URL=
VITE_WMS_STOKES_LAYERS=
VITE_WMS_STOKES_VERSION=1.3.0
VITE_WMS_STOKES_STYLES=

# Surry County, NC
VITE_WMS_SURRY_URL=
VITE_WMS_SURRY_LAYERS=
VITE_WMS_SURRY_VERSION=1.3.0
VITE_WMS_SURRY_STYLES=

# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=acme
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/acme?schema=public

# Redis
REDIS_URL=redis://localhost:6379

VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Example:
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
